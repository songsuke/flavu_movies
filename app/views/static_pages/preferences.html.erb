
  <%= javascript_include_tag "jquery.raty.min" %>
  <%= javascript_include_tag "jquery.raty" %>

<script type="text/javascript">
$(function () {
	$.fn.raty.defaults.path = '/assets/';
    $('#star').raty({
   	space: false,
   	starOff: 'rate_off.png',
  	starOn : 'rate_on.png',
  	width: 200,
    size: 1,
   	numberMax: 100,
   	number: 100,
  	targetType: 'number',
  	targetKeep: true
	});
})
</script>

<script type="text/javascript">
$(function () {
	$.fn.raty.defaults.path = '/assets/';
    $('#star_director').raty({
   	space: false,
   	starOff: 'rate_off.png',
  	starOn : 'rate_on.png',
  	width: 200,
    size: 1,
   	numberMax: 100,
   	number: 100,
  	targetType: 'number',
  	targetKeep: true
	});
})
</script>

<script type="text/javascript">
$(function () {
	$.fn.raty.defaults.path = '/assets/';
    $('#star_actor').raty({
   	space: false,
   	starOff: 'rate_off.png',
  	starOn : 'rate_on.png',
