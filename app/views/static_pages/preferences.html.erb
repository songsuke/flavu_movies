    size: 1,
   	numberMax: 100,
   	number: 100,
  	targetType: 'number',
  	targetKeep: true
	});
})
</script>

<div id="preference-content">
	<div id="myTabContent" class="tab-content">
    	<div id="genres_p" class="tab-pane active">
    		<%@gp.each do |gp1| %>
	    		<div class="preference-items">
	    			<div id="preference-picture">
	    				<img src="/assets/web_genre.png">
	    			</div>
	    			<div id="preference-name">
	    				 <%=gp1['genre']%>
	    			</div>
	    			<div id="preference-activities">
<script type="text/javascript">
	$(function () {
		$.fn.raty.defaults.path = '/assets/';
	   	$("#<%= gp1['genre'] %>").raty({
	   	space: false,
	   	starOff: 'rate_off.png',
	  	starOn : 'rate_on.png',
	  	width: 200,
	    size: 1,
	   	numberMax: 100,
	   	number: 100,
	  	score: function() {
	    return $(this).attr('data-score');
	  	}
		});
	})
</script>
	    				<div id="<%= gp1['genre'] %>" data-score="<%= gp1['score'] %>" style="height:30px;"></div>
	    				
	    				<div id="pcreate-button">
							<input name="create1" id="create1" type="submit" class="btn btn-primary" value="Create">
						</div>
						
					</div>
						
	    		</div>
			<%end%>
	    </div>
	
    		
    	<div id="directors_p" class="tab-pane">
    		<%@dp.each do |dp1| %>
	    		<div class="preference-items">
	    			<div id="preference-picture">
	    				<img src="/assets/web_director.png">
	    			</div>
	    			<div id="preference-name">
	    				 <%=dp1['director']%>
	    			</div>
	    			<div id="preference-activities">
	    				<div id="star" data-score="1"></div>
	    				<div id="pcreate-button">
							<input name="create1" id="create1" type="submit" class="btn btn-primary" value="Create">
						</div>
						
					</div>
						
	    		</div>
			<%end%>
    	</div>
    	<div id="actors_p" class="tab-pane">
    		<%@ap.each do |ap1| %>
	    		<div class="preference-items">
	    			<div id="preference-picture">
	    				<img src="/assets/web_actor.png">
	    			</div>
	    			<div id="preference-name">
	    				 <%=ap1['actor']%>
	    			</div>
	    			<div id="preference-activities">

	    				<div id="pcreate-button">
							<input name="create1" id="create1" type="submit" class="btn btn-primary" value="Create">
						</div>
						
					</div>
						
	    		</div>
			<%end%>
    	</div>
    </div>
</div>



